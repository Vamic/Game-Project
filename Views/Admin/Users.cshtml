@model IEnumerable<ApplicationUser>

<h3>Selected</h3>
<div id="user-edit-form">
    @Html.Partial("EditUser", new UserBindingModel())
</div>

<h3>Users</h3>
<table class="table table-bordered">
    <thead>
        <tr>
            <th>Display Name</th>
            <th>User Name</th>
            <th>Experience</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (ApplicationUser user in Model)
        {
            string rowId = "user-row-" + user.Id;
            <tr id="@rowId">
                @Html.Partial("_UserRow", user)
            </tr>
        }
    </tbody>
</table>
